{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs22.x"
    }
  },
  "builds": [
    { "src": "packages/backend/src/**/*.ts", "use": "@vercel/node" },
    { "src": "packages/backend/api/**/*.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" }
  ]
}

